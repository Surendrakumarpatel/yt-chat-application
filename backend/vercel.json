{
    "version":2,
    "builds":[
        {
            "src":"*.js",
            "use":"@vercel/node"
        }
    ],
    "routes":[
        {
            "src":"/(.*)",
            "dest":"index.js",
            "methods":["GET","POST"],
            "headers":{
                "Access-Control-Allow-Origin":"*"
            }
        }
    ]
}